          <max_envelope>1</max_envelope>
          <holes>
            <hole>50</hole>
          </holes>
        </autonumeration>
        <size>500</size>
        <rule>
          <idr>1</idr>
          <attribute>idp</attribute>
          <operation>greater</operation>
          <value>10</value>
        </rule>
        <gaps />
        <priority>4</priority>
      </storehouse>
      <storehouse>
        <ids>2</ids>
        <name>ZbytKrotkiPesel</name>
        <autonumeration>
          <max_envelope>1</max_envelope>
          <holes></holes>
        </autonumeration>
        <size>500</size>
        <rule>
          <idr>2</idr>
          <attribute>pesel</attribute>
          <operation>lesser</operation>
          <value>9999999999</value>
        </rule>
        <priority>5</priority>
        <rule>
          <idr>5</idr>
          <attribute>pesel</attribute>
          <operation>greater</operation>
          <value>500</value>
        </rule>
      </storehouse>
      <storehouse>
        <ids>3</ids>
        <name>NazwiskoPingot</name>
        <autonumeration>
          <max_envelope>1</max_envelope>
          <holes>
            <hole>5</hole>
          </holes>
        </autonumeration>
        <size>500</size>
        <rule>
          <idr>3</idr>
          <attribute>nazwisko</attribute>
          <operation>equal</operation>
          <value>Pingot</value>
        </rule>
        <priority>3</priority>
      </storehouse>
      <storehouse>
        <ids>4</ids>
        <name>W ciąży</name>
        <autonumeration>
          <max_envelope>1</max_envelope>
          <holes></holes>
        </autonumeration>
        <size>500</size>
        <rule>
          <idr>4</idr>
          <attribute>ciąża</attribute>
          <operation>equal</operation>
          <value>true</value>
        </rule>
        <priority>2</priority>
      </storehouse>
      <storehouse>
        <ids>5</ids>
        <name>id równe 15</name>
        <autonumeration>
          <max_envelope>1</max_envelope>
          <holes></holes>
        </autonumeration>
        <size>500</size>
        <rule>
          <idr>6</idr>
          <attribute>idp</attribute>
          <operation>equal</operation>
          <value>15</value>
        </rule>
        <priority>1</priority>
      </storehouse>
      <storehouse>
        <ids>6</ids>
        <name>Wysoki i w ciąży</name>
        <autonumeration>
          <max_envelope>1</max_envelope>
          <holes></holes>
        </autonumeration>
        <size>500</size>
        <rule>
          <idr>7</idr>
          <attribute>ciąża</attribute>
          <operation>equal</operation>
          <value>true</value>
        </rule>
        <rule>
          <idr>8</idr>
          <attribute>wzrost</attribute>
          <operation>greater</operation>
          <value>250</value>
        </rule>
        <priority>0</priority>
      </storehouse>
    </storehouses>
    <customfields>
      <customfield>
        <idf>1</idf>
        <fieldname>wzrost</fieldname>
        <fieldtype>int</fieldtype>
        <fielddefault>100</fielddefault>
